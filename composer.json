{
    "name": "siganushka/order-contracts",
    "description": "A set of abstractions for order struct.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "siganushka",
            "email": "siganushka@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/collections": "^1.6"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Siganushka\\Contracts\\Order\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Siganushka\\Contracts\\Order\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
